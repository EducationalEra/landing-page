{
  "name": "edera-landing",
  "version": "1.0.0",
  "description": "Landing Page for EdEra",
  "author": "Olga Filipova <chudaol@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=staging nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "NODE_ENV=production nuxt generate",
